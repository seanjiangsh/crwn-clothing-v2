{
  "name": "crwn-clothing-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "sls": "npm run dev ---prefix aws",
    "dev-sls": "concurrently \"npm run sls\" \"npm run gql-codegen\" \"npm run dev\"",
    "netlify": "netlify dev",
    "gql-codegen": "wait-on http-get://localhost:3000/graphql?query=%7B__typename%7D && graphql-codegen -c graphql/codegen.ts -w",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "install-sls": "npm install --prefix aws",
    "deploy-sls": "npm run install-sls && npm run deploy --prefix aws"
  },
  "dependencies": {
    "@apollo/client": "3.9.6",
    "@reduxjs/toolkit": "^2.2.1",
    "@stripe/react-stripe-js": "2.5.1",
    "@stripe/stripe-js": "3.0.7",
    "firebase": "10.8.1",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.22.2",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "stripe": "14.20.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@netlify/functions": "^2.6.0",
    "@parcel/watcher": "^2.4.1",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/aws-lambda": "8.10.136",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.3.1",
    "apollo-server-lambda": "^3.13.0",
    "concurrently": "^8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "netlify-cli": "17.19.0",
    "prettier": "3.2.5",
    "typescript": "5.4.2",
    "vite": "5.1.5",
    "vite-plugin-pwa": "0.19.2",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  }
}
