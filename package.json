{
  "name": "crwn-clothing-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-sls": "concurrently \"npm run netlify\" \"npm run gql-codegen\"",
    "netlify": "netlify dev",
    "gql-codegen": "wait-on http-get://localhost:8888/.netlify/functions/graphql?query=%7B__typename%7D && graphql-codegen -c graphql/codegen.ts -w",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@netlify/functions": "^2.6.0",
    "@reduxjs/toolkit": "^2.2.1",
    "@stripe/react-stripe-js": "^2.5.0",
    "@stripe/stripe-js": "^3.0.5",
    "dotenv": "^16.4.5",
    "firebase": "10.8.0",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.22.1",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "stripe": "14.18.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "@svgr/rollup": "^8.1.0",
    "@types/aws-lambda": "^8.10.134",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "apollo-server-lambda": "^3.13.0",
    "concurrently": "^8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "netlify-cli": "^17.17.0",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "vite": "5.1.4",
    "wait-on": "^7.2.0"
  }
}
